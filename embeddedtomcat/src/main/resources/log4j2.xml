<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
	<Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" charset="IBM850" />
     </Console>
  </Appenders>
  <Loggers>
    <Logger name="org.leplan73.outilssgdf" level="info" additivity="false">
      <AppenderRef ref="console"/>
    </Logger>
    <Root level="err">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>
</Configuration>
